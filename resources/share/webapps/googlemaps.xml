<?xml version="1.0" ?>
<webapp>
	<application>
		<title>Google Maps</title>
		<url>http://maps.google.co.uk</url>
		<geometry>
			<left>0</left>
			<top>0</top>
			<width>1024</width>
			<height>768</height>
		</geometry>
	</application>
	<domains>
		<default>external</default>
		<dropadverts>false</dropadverts>
		<rules>
			<external>http://maps.google.co.uk/local_url</external>
			<internal>http://maps.google</internal>
			<internal>https://www.google</internal>
			<internal>http://www.google.co.uk/intl/en_uk/help</internal>
			<internal>http://www.google.co.uk/help/maps</internal>
			<internal>http://gmodules</internal>
			<external>http://www.google</external>
			<external>http://images.google</external>
			<external>http://news.google</external>
			<external>http://groups.google</external>
			<external>http://scholar.google</external>
			<advert>http://adwords.google</advert>
		</rules>
	</domains>
</webapp>
