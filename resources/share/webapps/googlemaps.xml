<?xml version="1.0" ?>
<webapp>
	<application>
		<title>Google Maps</title>
		<url>http://maps.google.co.uk</url>
	</application>
	<domains>
		<rules>
			<external>http://maps.google.co.uk/local_url</external>
			<internal>http://maps.google</internal>
			<internal>https://www.google</internal>
			<internal>http://www.google.co.uk/intl/en_uk/help</internal>
			<internal>http://www.google.co.uk/help/maps</internal>
			<internal>http://gmodules</internal>
			<external>http://www.google</external>
			<external>http://images.google</external>
			<external>http://news.google</external>
			<external>http://groups.google</external>
			<external>http://scholar.google</external>
			<advert>http://adwords.google</advert>
		</rules>
	</domains>
	<interface>
		<menubar>
			<item label="Start Again" group="Application">http://maps.google.co.uk</item>
			<item label="Go Back a Page" group="Application">javascript:history.back()</item> 
			<item label="Quit" group="Application">medes:/earlydays/action/quit()</item>
			<item label="Sign In" group="Account">https://www.google.com/accounts/ServiceLogin?service=local&amp;hl=en&amp;nui=1&amp;continue=http://maps.google.co.uk</item>
			<item label="Saved Locations" group="Account">https://www.google.com/accounts/ServiceLogin?service=local&amp;hl=en&amp;nui=1&amp;continue=http://maps.google.co.uk</item>
			<item label="Maps Help" group="Help">http://maps.google.co.uk/support/?hl=en_GB</item>
			<item label="Take a Tour" group="Help">http://www.google.co.uk/intl/en_uk/help/maps/tour/</item>
			<item label="About Google Maps" group="Help">http://maps.google.co.uk/support/bin/topic.py?topic=10778</item>
		</menubar>
	</interface>
</webapp>
