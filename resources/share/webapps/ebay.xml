<?xml version="1.0" ?>
<webapp>
	<application>
		<title>eBay: The UK's Online Marketplace</title>
		<url>http://www.ebay.co.uk</url>
		<windowwidth>850</windowwidth>
		<windowheight>600</windowheight>
	</application>
	<rules>
		<domaindefault>external</domaindefault>
		<hideadverts>true</hideadverts>
		<domains>
			<internal>.ebayobjects.</internal>
			<internal>.ebay.</internal>
		</domains>
	</rules>
</webapp>
