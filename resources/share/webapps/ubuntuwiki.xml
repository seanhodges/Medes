<?xml version="1.0" ?>
<webapp>
	<application>
		<title>Ubuntu Community Documentation</title>
		<url>https://help.ubuntu.com/community/UserDocumentation</url>
	</application>
	<rules>
		<http>
			<internal>https://help.ubuntu.com</internal>
			<internal>https://wiki.ubuntu.com</internal>
		</http>
	</rules>
	<interface>
		<menubar>
			<item label="Start Again" group="Application">https://help.ubuntu.com/community/</item>
			<item label="Go Back a Page" group="Application">javascript:history.back()</item> 
			<item label="Preferences" group="Application">https://help.ubuntu.com/community/UserPreferences</item>
			<item label="Quit" group="Application">medes://earlydays/action/quit</item>
		</menubar>
	</interface>
</webapp>
