<?xml version="1.0" ?>
<webapp>
	<application>
		<title>Ubuntu Community Documentation</title>
		<icon>ubuntu.svg</icon>
		<url>https://help.ubuntu.com/community/UserDocumentation</url>
	</application>
	<rules>
		<http>
			<internal>https://help.ubuntu.com</internal>
			<internal>https://wiki.ubuntu.com</internal>
		</http>
	</rules>
	<interface>
		<menubar>
			<item label="_Start Again" group="_Application" shortcut="ctrl+s">https://help.ubuntu.com/community/</item>
			<separator group="_Application"/>
			<item label="_Preferences" group="_Application" shortcut="ctrl+p">https://help.ubuntu.com/community/UserPreferences</item>
			<separator group="_Application"/>
			<item label="Go _Back a Page" group="_Application">javascript:history.back()</item> 
			<item label="_Quit" group="_Application" shortcut="ctrl+q">medes://earlydays/action/quit</item>
		</menubar>
	</interface>
</webapp>
