<?xml version="1.0" ?>
<webapp>
	<application>
		<title>Facebook</title>
		<url>http://www.facebook.com</url>
		<windowwidth>800</windowwidth>
		<windowheight>600</windowheight>
	</application>
	<rules>
		<domaindefault>external</domaindefault>
		<dropadverts>false</dropadverts>
		<domains>
			<internal>https://login.facebook.com</internal>
			<advert>http://ads.ak.facebook.com/ads</advert>
			<internal>facebook.com</internal>
		</domains>
	</rules>
</webapp>
